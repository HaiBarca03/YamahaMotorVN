// popup_form_register
$(window).on('load', function(event) {
  if($('#popup_form_register').length){
    var wth = $(window).width();
    if(wth > 768) {
      setTimeout(function(){
        $('#popup_form_register').addClass('register-active');
      },2000)
    } else {
      setTimeout(function(){
        $('#popup_form_register').addClass('register-active');
      },5000)
    }
    $('.register-close').on('click', function(event) {
      $('#popup_form_register').removeClass('register-active');
    });
  }
});



